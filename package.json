{
  "name": "@automattic/vip-go-mu-plugins",
  "private": true,
  "version": "1.0.0",
  "description": "mu-plugins used on the VIP Go platform",
  "scripts": {
    "phplint": "bin/php-lint.sh",
    "phpcs": "vendor/bin/phpcs --cache",
    "phpcs:fix": "vendor/bin/phpcbf",
    "lint": "npm run phplint && npm run phpcs",
    "update-es-wp-query": "git subtree pull --prefix search/es-wp-query git@github.com:Automattic/es-wp-query master --squash",
    "prepare": "husky install",
    "test-e2e": "npx playwright test -c '__tests__/e2e/playwright.config.ts'",
    "test-e2e:gutenberg": "wp-scripts test-e2e --config ./node_modules/@wordpress/e2e-tests/jest.config.js --wordpress-base-url=http://localhost --wordpress-username=e2e_tester --wordpress-password=aut0matedTe$ter"
  },
  "author": "Automattic",
  "devDependencies": {
    "@playwright/test": "1.17.1",
    "asana-phrase": "0.0.8",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "phplint": "2.0.5",
    "playwright": "1.17.1"
  }
}
