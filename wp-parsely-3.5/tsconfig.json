{
  "compilerOptions": {
	"allowJs": true,
	"esModuleInterop": true,
	"jsx": "react-jsx",
	"module": "es6",
	"moduleResolution": "node",
	"noImplicitAny": true,
	"outDir": "./build/",
	"resolveJsonModule": true,
	"target": "es5",
	"typeRoots": ["./src/@types", "./node_modules/@types"],
  },
  "exclude": [
	"node_modules", "vendor"
  ]
}
